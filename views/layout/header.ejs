<header class="bg-white mb-5">
  <nav class="mx-auto max-w-full text-center p-6" aria-label="Global">
    <div class="flex flex-6 justify-center">
      <div class="flex col-6"></div>
      <div class="flex basis-full">
        <h1 class="italic text-dark max-w-full font-extrabold py-2 px-4 text-5xl">
          Fuzzar Tool
        </h1>
        <img src="./images/logoa.jpg" class="w-1/12">
      </div>
      <% if(profiles === undefined) { %>
      <div>
        <a href="/profile/execute">
          <p>
            <span class="inline-block align-middle">
              Realizar Prueba
            </span>
            <span class="inline-block align-middle">
              <svg class="ms-3" width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M7 14.3333C7 13.0872 7 12.4641 7.26795 12C7.44349 11.696 7.69596 11.4435 8 11.2679C8.4641 11 9.08718 11 10.3333 11H13.6667C14.9128 11 15.5359 11 16 11.2679C16.304 11.4435 16.5565 11.696 16.7321 12C17 12.4641 17 13.0872 17 14.3333V16C17 16.9293 17 17.394 16.9231 17.7804C16.6075 19.3671 15.3671 20.6075 13.7804 20.9231C13.394 21 12.9293 21 12 21V21C11.0707 21 10.606 21 10.2196 20.9231C8.63288 20.6075 7.39249 19.3671 7.07686 17.7804C7 17.394 7 16.9293 7 16V14.3333Z" stroke="#33363F" stroke-width="2"/>
                <path d="M9 9C9 8.06812 9 7.60218 9.15224 7.23463C9.35523 6.74458 9.74458 6.35523 10.2346 6.15224C10.6022 6 11.0681 6 12 6V6C12.9319 6 13.3978 6 13.7654 6.15224C14.2554 6.35523 14.6448 6.74458 14.8478 7.23463C15 7.60218 15 8.06812 15 9V11H9V9Z" stroke="#33363F" stroke-width="2"/>
                <path d="M12 11V15" stroke="#33363F" stroke-width="1"/>
                <path d="M15 3L13 6" stroke="#33363F" stroke-width="1"/>
                <path d="M9 3L11 6" stroke="#33363F" stroke-width="1"/>
                <path d="M7 16H2" stroke="#33363F" stroke-width="1"/>
                <path d="M22 16H17" stroke="#33363F" stroke-width="1"/>
                <path d="M20 9V10C20 11.6569 18.6569 13 17 13V13" stroke="#33363F" stroke-width="1"/>
                <path d="M20 22V22C20 20.3431 18.6569 19 17 19V19" stroke="#33363F" stroke-width="1"/>
                <path d="M4 9V10C4 11.6569 5.34315 13 7 13V13" stroke="#33363F" stroke-width="1"/>
                <path d="M4 22V22C4 20.3431 5.34315 19 7 19V19" stroke="#33363F" stroke-width="1"/>
              </svg>
            </span>
          </p>
        </a>
      </div>
      <div class="flex basis-1/12">
        <form action="/csv/download" method="get">
          <button
          class="group relative h-fit w-60 py-3 px-3 rounded-lg <%=analysis.length !== 0 ? 'bg-green-500 hover:text-white opacity-100' : '' %> shadow <%=analysis.length === 0 ? 'bg-gray-300 opacity-50' : 'hover:bg-green-600 hover:opacity-100' %>"
          id="export"
          <%=analysis.length === 0 ? 'disabled' : '' %>
          >
            <p>
              <span class="inline-block align-middle">
                Exportar datos a CSV
              </span>
              

              <span class="inline-block align-middle">
                <svg
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg"
                  class="ms-3"
                >
                  <path
                    d="M14 4H20M20 4V10M20 4L12 12"
                    stroke="#33363F"
                    stroke-width="2"
                  ></path>
                  <path
                    d="M11 5H7C5.89543 5 5 5.89543 5 7V17C5 18.1046 5.89543 19 7 19H17C18.1046 19 19 18.1046 19 17V13"
                    stroke="#33363F"
                    stroke-width="2"
                    stroke-linecap="round"
                  ></path>
                </svg>
              </span>
            </p>
          </button>
        </form>
      </div>
      <% } %>
    </div>
  </nav>
  <hr />
</header>